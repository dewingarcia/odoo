            </t>
            <t t-else="">
                <link rel="stylesheet" type="text/less" href="/web/static/src/less/ltr.less"/>
            </t>
            <link rel="stylesheet" type="text/less" href="/web/static/src/less/mixin_rtl_ltr.less"/>
            <link rel="stylesheet" type="text/less" href="/point_of_sale/static/src/less/pos.less" id="pos-stylesheet"/>
